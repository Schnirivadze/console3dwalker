<settings>
    <fps>30</fps><!--Кадры в секуну-->
    <pov>60</pov><!--Поле зрения-->
    <steppov>0.5</steppov><!--Шаг рендера-->
    <speed>0.1</speed><!--Скорость-->
    <rangeofview>15</rangeofview><!--Дальность зрения-->
    <screenheight>30</screenheight><!--Высота экрана-->
    <mapinfo>
        <seed>random</seed><!--Сид карты, можно вписать random для рандомного сида-->
        <size>50</size><!--Размер карты-->
        <rooms>20</rooms><!--Количество комнат-->
        <minroomsize>7</minroomsize><!--Минимальный размер комнаты-->
        <maxroomsize>10</maxroomsize><!--Максимальный размер комнаты-->
    </mapinfo>
</settings>
    